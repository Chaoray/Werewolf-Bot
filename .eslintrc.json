{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "object-curly-spacing": [
            "off"
        ],
        "comma-dangle": [
            "error",
            {
                "objects": "always",
                "arrays": "always"
            }
        ],
        "max-len": "off",
        "require-jsdoc": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "local",
                "args": "after-used",
                "ignoreRestSiblings": false
            }
        ]
    }
}